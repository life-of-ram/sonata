    <script>

        $("#section1").css("display", "block")
        $("#section2").css("display", "none")

        function scrollToTop() {
            window.scrollTo({ top: 0, behavior: 'smooth' });
        }

        function next() {
            var allQuestionsAnswered = true;
            var firstUnansweredQuestion = null;

            for (var i = 1; i <= 36; i++) {
                var radioElements = document.querySelectorAll('[name="q' + i + '"]');
                var isQuestionAnswered = false;

                radioElements.forEach(function (radioElement) {
                    if (radioElement.checked) {
                        isQuestionAnswered = true;
                    }
                });

                if (!isQuestionAnswered) {
                    allQuestionsAnswered = false;
                    if (!firstUnansweredQuestion) {
                        firstUnansweredQuestion = radioElements[0];
                    }
                }
            }

            if (!allQuestionsAnswered && firstUnansweredQuestion) {
                firstUnansweredQuestion.scrollIntoView({ behavior: 'smooth', block: 'center' });
                var unansweredQuestionContainers = document.querySelectorAll('.likert:not([style*="background: none"])');
                unansweredQuestionContainers.forEach(function (container) {
                    var inputs = container.querySelectorAll('.options');
                    var hasCheckedInput = Array.from(inputs).some(function (input) {
                        return input.checked;
                    });

                    if (!hasCheckedInput) {
                        container.classList.add('unanswered');
                        setTimeout(function () {
                            container.classList.remove('unanswered');
                        }, 2000); // Adjust the duration (in milliseconds) as needed
                    } else {
                        container.classList.remove('unanswered');
                    }
                })
            } else {
                $("#section1").css("display", "none")
                $("#section2").css("display", "block")
                $("#next").css("display", "none");
                $("#prev").css("display", "block");
                scrollToTop();
            }
        }
        function prev() {
            $("#section1").css("display", "block")
            $("#section2").css("display", "none")
            $("#next").css("display", "block");
            $("#prev").css("display", "none");
        }



        function disabledBackbutton() {
            window.history.forward();
        }
        document.querySelector("form").addEventListener("submit", disabledBackbutton);

        function validateForm(event) {
            event.preventDefault(); // Prevent the form from submitting

            var selectedValue1 = document.querySelector('select[name="gender"]').value;
            var selectedValue2 = document.querySelector('select[name="years"]').value;
            var selectedValue3 = document.querySelector('select[name="age"]').value;
            var selectedValue4 = document.querySelector('select[name="location"]').value;
            var selectedValue5 = document.querySelector('select[name="unit"]').value;
            var selectedValue6 = document.querySelector('select[name="band"]').value;





            if (selectedValue1 === "Please select" || selectedValue2 === "Please select" || selectedValue3 === "Please select" || selectedValue4 === "Please select" || selectedValue5 === "Please select" || selectedValue6 === "Please select") {
                alert("Please select all options");
                window.scrollTo(0, 0); // Scroll to the top of the page
            } else {
                document.getElementById("managementForm").submit(); // Submit the form
            }
        }


    </script>
